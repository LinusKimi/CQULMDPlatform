{
  "systemConfig": {
    // 厂商 
    "Factory": "云鲸 - ST",

    // 软件版本
    "SoftwareVersion": "V_TestPlatform",

    // 下位机接口 IIC / UART
    "BoardInterface": "IIC",

    // IIC 速率
    "IICBaud": "100000",

    // 串口 波特率 （8 - N - 1 暂时默认）
    "UARTBaud": "115200",

    // 芯片类型
    "MCU": "STM32G031G6",

    // 芯片烧写Flash首地址
    "FlashAddress": "0x08000000",

    // 烧结文件名
    "BinFileName": "cq_ul_md_yj_v3.bin"
  },
  "ConfigurationNode": {
    // 单次指令
    "SignalCMD": "55 AA 0D 0C",

    // 单次指令回复数据长度
    "ReturnSignalCMD": 18,

    // 连续指令
    //"ContinualCMD": "55 AA 0D 0C",

    // 停止连续
    //"StopContCMD": "55 AA 0D 0C",

    // 内部版本
    "InnerVersion": "55 AA 89 45",

    // 复位指令 "" / null 代表没有该项
    "ResetDev": "55 AA FF FF",

    // AE 指令
    "USEAE": "55 AA AE FF",

    // EA 指令
    //"USEEA": "",

    // 是否使用 AE/EA 指令
    "EnableAEEA": false,

    // 空板电流 [低, 高]
    "EmptyCurrentValue": [ 5.0, 30.0 ],

    // 运行电流 [低, 高]
    "BoardCurrentValue": [ 12.0, 18.0 ]
  }
 }