{
  "systemConfig": {
    // 厂商
    "Factory": "YJ",

    // 软件版本
    "SoftwareVersion": "V_TestPlatform",

    // 下位机接口 IIC / UART
    "BoardInterface": "IIC",

    // IIC 速率
    "IICBaud": "100000",

    // 串口 波特率 （8 - N - 1 暂时默认）
    "UARTBaud": "115200",

    // 芯片类型
    "MCU": "STM32G0",

    // 芯片烧写Flash首地址
    "FlashAddress": "0x0000000",

    // 烧结文件名
    "BinFileName": ".bin"
  },
  "ConfigurationNode": {
    // 单次指令
    "SignalCMD": "55 AA 0D 0C",

    // 连续指令
    "ContinualCMD": "55 AA 0D 0C",

    // 停止连续
    "StopContCMD": "55 AA 0D 0C",

    // 内部版本
    "InnerVersion": "55 AA 89 45",

    // 复位指令 "" / null 代表没有该项
    "ResetDev": "",

    // AE 指令
    "USEAE": "",

    // EA 指令
    "USEEA": "",

    // 是否使用 AE/EA 指令
    "EnableAEEA": false,

    // 空板电流 [低, 高]
    "EmptyCurrentValue": [ 0, 1 ],

    // 运行电流 [低, 高]
    "BoardCurrentValue": [ 0, 1 ]
  }
 }